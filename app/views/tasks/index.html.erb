<h1>Here are all the tasks</h1>
<% @tasks.each do |task| %>
  <h3><%= task.name %></h3>
  <p><%#= task.details %></p>
  <p><%#= task.completed %></p>
  <%= link_to "See details", task_path(task) %>
  <%= link_to "Edit this task", edit_task_path(task)%>
  <%= link_to "Delete this task", task_path(task), method: :delete %>
<% end %>
